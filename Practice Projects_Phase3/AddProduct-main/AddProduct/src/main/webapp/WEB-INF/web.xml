<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
	<display-name>AddProduct</display-name>
	<welcome-file-list>
		<welcome-file>>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>PetsServlet</display-name>
		<servlet-name>PetsServlet</servlet-name>
		<servlet-class>com.example.PetsServlet</servlet-class>
	</servlet>

	<servlet>
		<description>
			</description>
		<display-name>AddedPrompt</display-name>
		<servlet-name>AddedPrompt</servlet-name>
		<servlet-class>com.example.AddedPrompt</servlet-class>
	</servlet>



	<servlet-mapping>
		<servlet-name>PetsServlet</servlet-name>
		<url-pattern>/pets</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>AddedPrompt</servlet-name>
		<url-pattern>/AddedPrompt</url-pattern>
	</servlet-mapping>

</web-app>	